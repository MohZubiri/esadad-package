{
    "name": "esadad/payment-gateway",
    "description": "Laravel package for e-SADAD payment gateway integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "ext-soap": "*"
    },
    "autoload": {
        "psr-4": {
            "ESadad\\PaymentGateway\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ESadad\\PaymentGateway\\Providers\\ESadadServiceProvider"
            ],
            "aliases": {
                "ESadad": "ESadad\\PaymentGateway\\Facades\\ESadad"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
